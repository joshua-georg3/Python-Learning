from tkinter import *

root = Tk()

# Creating a label widget 
myLabel1 = Label(root, text="Hello World")
myLabel2 = Label(root, text="My name is Joshua")
myLabel3 = Label(root, text="I'm learning tkinter")

# Placing it on a grid
myLabel1.grid(row=0, column=0)
myLabel2.grid(row=3, column=0)
myLabel3.grid(row=5, column=0)


root.mainloop()
