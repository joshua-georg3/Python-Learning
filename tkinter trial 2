from tkinter import *
from PIL import Image,ImageTk


root = Tk()
root.title("Trial Window")
root.iconbitmap('E:/Joshua/boy.ico')


my_img = ImageTk.PhotoImage(Image.open("E:/Joshua/Personal/icons n stuff/totoro.jpg"))
my_Label = Label(image=my_img)

my_Label.pack()


button_quit = Button(root, text="Exit Program", command=root.quit)
button_quit.pack()


root.mainloop()